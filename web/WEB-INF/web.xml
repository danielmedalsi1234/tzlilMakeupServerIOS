<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>PushContact</servlet-name>
        <servlet-class>danielmedalsi1234.com.PushContact</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PushContact</servlet-name>
        <url-pattern>/PushContact</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletImage</servlet-name>
        <servlet-class>danielmedalsi1234.com.ServletImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletImage</servlet-name>
        <url-pattern>/ServletImage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PullRequest</servlet-name>
        <servlet-class>danielmedalsi1234.com.PullRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PullRequest</servlet-name>
        <url-pattern>/PullRequest</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>push_image.html</welcome-file>
    </welcome-file-list>

</web-app>